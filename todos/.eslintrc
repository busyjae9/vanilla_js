{
  "root": true,
  "extends": [
    "eslint:recommended"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "env": {
    "es6": true
  },
  "globals": {
    "window": true,
    "document": true,
    "history": true,
    "FormData": true,
    "process": true,
    "console": true,
    "URLSearchParams": true,
    "URL": true,
    "Notification": true,
    "setInterval": true,
    "navigator": true,
    "localStorage": true,
    "sessionStorage": true,
    "self": true
  },
  "rules": {
    "semi": "warn",
    "max-len": [
      1,
      {
        "code": 100
      }
    ],
    "no-unused-vars": "warn"
  }
}
