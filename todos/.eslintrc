{
  "root": true,
  "extends": [
    "eslint:recommended"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "env": {
    "es6": true
  },
  "globals": {
    "window": true,
    "document": true,
    "history": true,
    "FormData": true,
    "process": true,
    "console": true
  },
  "rules": {
    "semi": "warn"
  }
}
